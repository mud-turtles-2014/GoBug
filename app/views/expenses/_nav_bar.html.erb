 <nav>
    <div class="navbar navbar-inverse navbar-fixed-top" role:"navigation">
      <div class="container-fluid">
          <div class="navbar-header"><%= link_to "GoBug!", root_path, class: "navbar-brand" %></div>
            <ul class="left">
              <%= search_form_for @search do |f| %>
              <div class="search_results">
              <li><%= f.text_field :description, placeholder: " Search Keyword" %></li><p>
              <li><%= f.text_field :location_name_cont, placeholder: " Location", class: "location" %><p></li>
              <li><%= f.text_field :usd_cost_gteq, placeholder: " Price from", class: "currency"%></li>
              <li><%= f.text_field :usd_cost_lteq, placeholder: " Price to" , class: "currency" %></li>
              <li><%= f.collection_select :category_id_eq, Category.all, :id, :name, include_blank: 'By Category' %></li>
              <li><%= f.submit "Search" %></li>
              </div>
              <% end %>
            </ul>
            <ul class="nav navbar-nav navbar-right">
              <% flash.each do |name, msg| -%>
                <%= content_tag :li, msg, class: "alert alert-#{name}" %>
                <% end -%>
                <li><%= link_to "Browse", expenses_path %></li>
            <% if session[:user_id] %>
                <li><%= link_to "My Dashboard", user_path(User.find(session[:user_id])) %></li>
                <li><%= link_to "Log Out", '/logout', method: :delete %></li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
  </nav>